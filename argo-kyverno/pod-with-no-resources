apiVersion: v1
kind: Pod
metadata:
  labels:
    run: no-limits
  name: no-limits
spec:
  containers:
  - image: nginx
    name: no-limits
    resources: 
      requests:
            memory: "64Mi"
            cpu: "250m"
          limits:
            memory: "128Mi"
            cpu: "500m"
  dnsPolicy: ClusterFirst
  restartPolicy: Always
status: {}
